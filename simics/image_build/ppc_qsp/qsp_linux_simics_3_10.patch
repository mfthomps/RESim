+static void qsp_timer_init(void)
+{
+
+	int irq;
+	struct device_node *np;
+	void __iomem *timer_base;
+
+	pr_info(KERN_INFO "qsp timer init\n");
+
+	np = of_find_compatible_node(NULL, NULL, "qsp,timer");
