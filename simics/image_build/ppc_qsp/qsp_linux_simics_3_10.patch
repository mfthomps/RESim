+	for (hw_cpu = 0; hw_cpu < QSP_MAX_CORES; ++hw_cpu) {
+		writel(hw_cpu, sysregs_base + PROBE_REG);
+		if (readl(sysregs_base + CPU_STATUS_REG) == 1)
