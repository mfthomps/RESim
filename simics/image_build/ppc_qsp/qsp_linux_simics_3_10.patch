+
+	timer_base = of_iomap(np, 0);
+	WARN_ON(!timer_base);
+	pr_debug("Timer base %p\n", timer_base);
+
+	irq = irq_of_parse_and_map(np, 0);
+	WARN_ON(irq == 0);
+
+	qsp_timer_init_one(irq, timer_base);
+}
